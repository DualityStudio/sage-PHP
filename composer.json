{
    "name": "nicoljamie/sage-php",
    "description": "A simple Sage SDK in PHP",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "NicolJamie",
            "email": "jamie.nicol@me.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "NicolJamie\\SagePHP\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NicolJamie\\SagePHP\\SageServiceProvider.php"
            ]
        }
    },
    "minimum-stability": "dev"
}
